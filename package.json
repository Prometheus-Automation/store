{
  "name": "prometheus-automation-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "security:audit": "npm audit --production && snyk test",
    "security:monitor": "snyk monitor",
    "db:backup": "pg_dump $DATABASE_URL > backup-$(date +%Y%m%d).sql",
    "db:migrate": "npm run db:backup && psql $DATABASE_URL < migration.sql",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "analyze": "ANALYZE=true next build",
    "audit:prod": "npm audit --production"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.120.3",
    "@stripe/react-stripe-js": "2.4.0",
    "@stripe/stripe-js": "2.4.0",
    "@supabase/auth-helpers-nextjs": "0.8.7",
    "@supabase/supabase-js": "2.39.0",
    "@tanstack/react-query": "5.17.19",
    "@upstash/ratelimit": "1.0.0",
    "@upstash/redis": "1.28.0",
    "acorn": "8.11.2",
    "clsx": "2.1.0",
    "date-fns": "3.2.0",
    "framer-motion": "11.0.3",
    "fuse.js": "7.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "lru-cache": "10.2.0",
    "lucide-react": "0.294.0",
    "next": "^14.2.30",
    "next-themes": "0.2.1",
    "python-ast": "0.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-hook-form": "7.62.0",
    "react-hot-toast": "2.5.2",
    "react-intersection-observer": "9.16.0",
    "react-player": "3.3.1",
    "swr": "2.2.4",
    "tailwind-merge": "2.2.0",
    "tailwindcss": "3.4.1",
    "web3": "4.3.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.1.0",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/acorn": "6.0.0",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash": "4.17.20",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.4",
    "postcss": "8.4.33",
    "sharp": "0.34.3",
    "snyk": "^1.1298.1",
    "supertest": "^7.1.4",
    "typescript": "5.3.3"
  }
}
