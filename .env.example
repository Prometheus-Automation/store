# Environment Variables Template - AI Marketplace
# Copy this to .env and replace with your actual values from Google Cloud secrets

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_APP_NAME="AI Marketplace"
VITE_APP_DESCRIPTION="Premium AI models marketplace"

# Supabase Configuration
# These values are stored in Google Cloud Secret Manager with the same names
VITE_SUPABASE_URL=your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration  
# Stored in Google Cloud Secret Manager
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# OAuth Configuration (configured in Supabase dashboard)
# Google OAuth: Configure in Google Cloud Console
# GitHub OAuth: Configure in GitHub Developer Settings

# Optional: Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=GA-MEASUREMENT-ID
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Development Settings
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_LIVE_CHAT=false
VITE_ENABLE_A_B_TESTING=false